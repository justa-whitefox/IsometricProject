<?xml version="1.0" encoding="utf-8"?>
<objects>

  <!-- todo:jason review Projectile PassByVerb settings (can search for PassByVerb) -->
  
  <!--

    Render Layers
    ************************
    7 - Overlay (Gas)
    6 - Creature Decal (Bloodsplatter)
    5 - Creature
    4 - Object (Table)
    3 - Wall Decal (Bloodsplatter)
    2 - Wall
    1 - Floor Decal (Liquid)
    0 - Floor
    ************************

  -->

  <object Name="Object">
    <part Name="Physics" Conductivity="0" IsReal="true" Solid="false" Weight="0" />
  </object>
  <object Name="*PooledObject" Inherits="Object">
  </object>
  <object Name="CosmeticObject" Inherits="Object">
    <part Name="Render" DisplayName="[Object]" RenderString="?" RenderLayer="0" RenderIfDark="true" IsReal="true" />
    <part Name="Physics" Conductivity="1" IsReal="true" Solid="false" Weight="-1" />
    <part Name="NoDamage" />
    <tag Name="NoEffects" />
    <tag Name="Cosmetic" />
    <tag Name="Gender" Value="neuter" />
    <intproperty Name="Inorganic" Value="1" />
    <tag Name="BaseObject" Value="*noinherit" />
    <tag Name="Immutable" />
  </object>  
  
</objects>
